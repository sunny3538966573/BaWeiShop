<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bwie.baweishop">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.front" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:name="com.bwie.app.MyAPP"
        android:allowBackup="true"
        android:icon="@drawable/gou"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.bwie.actvity.MainActivity" />
        <activity android:name="com.bwie.actvity.LoginActivity" />
        <activity android:name="com.bwie.actvity.RegisteActivity" />
        <activity android:name="com.bwie.actvity.DetailsActivity" />
        <activity android:name="com.bwie.actvity.InformationActivity" />
        <activity android:name="com.bwie.actvity.CircletActivity" />
        <activity android:name="com.bwie.actvity.FootActivity" />
        <activity android:name="com.bwie.actvity.AddressActivity" />
        <activity android:name="com.bwie.actvity.SearchShopActivity" />
        <activity android:name="com.bwie.actvity.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.bwie.actvity.AddAddressActivity" />
        <activity android:name="com.bwie.actvity.ExitActivity" />
        <activity android:name="com.bwie.actvity.PostActivity" />
        <activity android:name="com.bwie.actvity.ImgActivity" />
        <activity android:name="com.bwie.actvity.PayActivity" />
        <activity android:name="com.bwie.actvity.MyAddressActivity"></activity>
    </application>

</manifest>